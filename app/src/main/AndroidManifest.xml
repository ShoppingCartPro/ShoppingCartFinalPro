<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hsfb.ullauri.finalproject" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".EnterBudget"
            android:label="@string/app_name" >
            <intent-filter>`
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ShoppingCart"
            android:label="@string/title_activity_scan_items"
            android:parentActivityName=".EnterBudget" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.hsfb.ullauri.finalproject.EnterBudget" />
        </activity>

    </application>

</manifest>
